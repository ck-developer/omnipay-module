{
    "name": "ck-developer/omnipay-module",
    "description": "Omnipay module for Zend Framework 2",
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "claude khedhiri",
            "email": "claude@khedhiri.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "zendframework/zendframework": "2.*",
        "omnipay/common": "~2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "omnipay/paypal": "~2.0",
        "symfony/var-dumper": "~3.0"
    },
    "autoload": {
        "psr-0": {
            "OmnipayModule": "src/"
        }
    },
    "autoload-dev": {
        "psr-0": {
            "OmnipayModuleTest": "tests/"
        }
    },
    "suggest": {
        "omnipay/paypal": "~2.0"
    },
    "config": {
        "bin-dir": "bin",
        "preferred-install": "dist"
    }
}
